{
  "name": "highcharts-utils",
  "version": "1.0.0",
  "description": "",
  "main": "sl-select.js",
  "scripts": {
    "publish": "npm run minify & npm run git",
    "start": "live-server --port=7001",
    "minify": "npm run minifySelect & npm run minifyLegend & npm run minifyCommon",
    "minifySelect": "uglifyjs sl-select.js --compress --mangle --warn --output dist/sl-select.min.js",
    "minifyLegend": "uglifyjs reverse-legend.js --compress --mangle --warn --output dist/reverse-legend.min.js",
    "minifyCommon": "uglifyjs common.js --compress --mangle --warn --output dist/common.min.js",
    "git": "git add . && git commit -m 'some-message' && git push --all && gitDist",
    "gitDist": "git subtree push --prefix dist origin gh-pages",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yordanova/highcharts-utils.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yordanova/highcharts-utils/issues"
  },
  "homepage": "https://github.com/yordanova/highcharts-utils#readme",
  "dependencies": {
    "live-server": "^1.2.1",
    "uglify-js": "^3.13.8"
  }
}
