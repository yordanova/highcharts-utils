{
  "name": "highcharts-utils",
  "version": "1.0.0",
  "description": "",
  "main": "sl-select.js",
  "scripts": {
    "run": "npm run git",
    "minify": "npm run minify1 & npm run minify2 & npm run minify3",
    "minify1": "uglifyjs sl-select.js --compress --mangle --warn --output dist/sl-select.min.js",
    "minify2": "uglifyjs reverse-legend.js --compress --mangle --warn --output dist/reverse-legend.min.js",
    "minify3": "uglifyjs common.js --compress --mangle --warn --output dist/common.min.js",
    "git": "git add . && git commit -m 'some-message' && git push --all",
    "gitA": "git add . && git commit -m && npm run gitpush",
    "gitpush": "git push --all",
    "production": "npm run version:bump --silent && rm -f scripts.min.js && npm run minifyJS --silent && npm run version:add --silent",
    "version:bump": "npm version patch --no-git-tag-version --silent",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yordanova/highcharts-utils.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yordanova/highcharts-utils/issues"
  },
  "homepage": "https://github.com/yordanova/highcharts-utils#readme",
  "dependencies": {
    "uglify-js": "^3.13.8"
  }
}
